{
  "manifest_version": 3,
  "name": "ECHO Translate",
  "description": "翻译",
  "version": "1.0",
  "content_scripts": [{
    "matches": ["https://*/*", "http://*/*"],
    "js": ["scripts/content.js"]
  }],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}